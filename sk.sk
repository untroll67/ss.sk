command /finalban [<offline player>] [<text>]:
	permission: {@Modo}
	permission message: Unknow Command. Type "/help" for help.
	trigger:
		if arg 1 is set:
			if arg 2 is "Macro / Jitterclick / ... (permanent)":
				if arg 1 have permission "{@Modo}" or "{@Modo}" or "{@Helper}" or "BUILDER" or "op":
					send "{@prefix} &cVous ne pouvez pas ban un membre du staff !"
					stop
				else:
					set {mute.%arg 1%} to true
					send "{@prefix} &6Application de la sanction..." to sender
					wait 5 ticks
					send "{@prefix} &6Sanction appliquée." to sender
					execute console command "/tempban %arg 1% 15 d Macro / Jitterclick / ... (perm)"
			if arg 2 is "Macro / Jitterclick / ...":
				if arg 1 have permission "{@Modo}" or "{@Modo}" or "{@Helper}" or "BUILDER" or "op":
					send "{@prefix} &cVous ne pouvez pas ban un membre du staff !"
					stop
				else:
					set {mute.%arg 1%} to true
					send "{@prefix} &6Application de la sanction..." to sender
					wait 5 ticks
					send "{@prefix} &6Sanction appliquée." to sender
					execute console command "/tempban %arg 1% 15 d Macro / Jitterclick / ..."
			if arg 2 is "Anti-Knockback":
				if arg 1 have permission "{@Modo}" or "{@Modo}" or "{@Helper}" or "BUILDER" or "op":
					send "{@prefix} &cVous ne pouvez pas ban un membre du staff !"
					stop
				else:
					set {mute.%arg 1%} to true
					send "{@prefix} &6Application de la sanction..." to sender
					wait 5 ticks
