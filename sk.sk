			if arg 2 is "Reach":
				if arg 1 have permission "{@Modo}" or "{@Modo}" or "{@Helper}" or "BUILDER" or "op":
					send "{@prefix} &cVous ne pouvez pas ban un membre du staff !"
					stop
				else:
					set {mute.%arg 1%} to true
					send "{@prefix} &6Application de la sanction..." to sender
					wait 5 ticks
					send "{@prefix} &6Sanction appliquée." to sender
					execute console command "/tempban %arg 1% 15 d Reach"
			if arg 2 is "Autres":
				if arg 1 have permission "{@Modo}" or "{@Modo}" or "{@Helper}" or "BUILDER" or "op":
					send "{@prefix} &cVous ne pouvez pas ban un membre du staff !"
					stop
				else:
					set {mute.%arg 1%} to true
					send "{@prefix} &6Application de la sanction..." to sender
					wait 5 ticks
					send "{@prefix} &6Sanction appliquée." to sender
					execute console command "/tempban %arg 1% 15 d Autres"
